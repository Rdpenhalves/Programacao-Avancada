/******************************************************************************
  Rodrigo Dante Penhalves 15/09/22 - Imprime um Quadrado de Asteriscos nxn
*******************************************************************************/
#include <stdio.h>
void imprimeRetangulo (int n) 
{
    for (int l=1; l<=n; l++)
    {
        for (int c=1; c<=n * 5; c++)
            printf ("*");
            
        printf ("\n");
    }
}

int main()
{
    int n;
    //Entrada
    printf("Qual o tamanho desejado: \n");
    scanf("%d", &n);
    //SaÃ­da
    printf("Quadrado de %d\n", n);
    imprimeRetangulo(n);
    
    return(0);
}
